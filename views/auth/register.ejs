<%- include('../partials/boilerplate'); %>
<div class="user-container">
    <form method="POST" action="/user">
        <input class="register" name="username" type="text" placeholder="Username" required />
        <input class="register" name="email" type="email" placeholder="Email" required />
        <input class="register" name="password" type="password" placeholder="Password" required />
        <input class="register" name="name" type="text" placeholder="Name" required />
        <input type="submit" value="Register" />
    </form>
</div>
<%- include('../partials/end'); %>